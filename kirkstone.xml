<?xml version="1.0" encoding="UTF-8"?>

<manifest>
<default sync-j="4"/>

<remote name="yoctoproject" fetch="https://git.yoctoproject.org"/>
<remote name="oe"           fetch="https://git.openembedded.org"/>
<remote name="homeassist"   fetch="https://github.com/bachp"/>


<project name="poky"                remote="yoctoproject"    revision="kirkstone" path="sources/poky"/>
<project name="meta-security"       remote="yoctoproject"    revision="kirkstone" path="sources/poky/meta-security"/>
<project name="meta-raspberrypi"    remote="yoctoproject"    revision="kirkstone" path="sources/poky/meta-raspberrypi"/>
<project name="meta-openembedded"   remote="oe"              revision="kirkstone" path="sources/poky/meta-openembedded"/>
<project name="meta-homeassistant"  remote="homeassist"      revision="kirkstone" path="sources/poky/meta-homeassistant"/>
</manifest>
