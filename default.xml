<project name="meta-raspberrypi"    remote="yoctoproject"    path="poky/meta-raspberrypi"/>
<project name="meta-openembedded"   remote="oe"              path="poky/meta-openembedded"/>
<project name="meta-homeassistant"  remote="homeassist"      path="poky/meta-homeassistant"/>
</manifest>
